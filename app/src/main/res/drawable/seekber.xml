<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:startColor="#DEE2EB"
                android:centerColor="#DEE2EB"
                android:endColor="#DEE2EB" />
            <corners android:radius="100dip"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#0B83FE"
                    android:centerColor="#0B83FE"
                    android:endColor="#0B83FE" />
                <corners android:radius="100dip"/>
            </shape>
        </clip>
    </item>

</layer-list>